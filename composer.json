        "ext-zip": "*",
        "ext-fileinfo": "*",
        "webklex/php-imap": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload-dev": {
        "psr-4": {
