        "webklex",
        "imap",
        "pop3",
        "php-imap",
        "mail"
    ],
    "homepage": "https://github.com/webklex/php-imap-github-actions",
    "license": "MIT",
    "authors": [
        {
            "name": "Malte Goldenbaum",
            "email": "github@webklex.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "ext-openssl": "*",
